<h1 class="text-center">All Events</h2>
<% @events.each do |event| %>
<h2><a href="/events/<%=event.id%>">Event Name: <%= event.name %></a></h1>

<h4>Event Type: <%= event.event_type %></h4>
<h4>Event Date: <%= event.location %></h4>
<h4>Event Time: <%= event.event_date %></h4>
<h4>Event Date: <%= event.event_time %></h4>
<h4>Event Date: <%= event.event_uri %></h4>
<h4>Event Description: <%= event.description %></h4>
<% end %>
